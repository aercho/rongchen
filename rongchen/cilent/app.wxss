/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 

.lf{
  float: left;
}

.rt{
  float: right;
}

.clearfix {
  overflow: auto;
  _height: 1%;
}

text{
  font-family: "Microsoft Yahei";
}
.getline{
    background-color: #CDC8B1;
    height: 10rpx;
}
.getSmallline{
  background-color: #CDC8B1;
  height: 4rpx;
}
view{
  color: #2E2E2E;
}


/*搜索框样式*/
.search-border{
    border-radius: 20%;
    width: 600rpx;
    height: 64rpx;
    text-align: center;
    position: fixed;
    left: 10%;
}
.searchBox{
    background: #6B8E23;
    height: 80rpx;
}
.search{
    width: 100%;
    display: inline-block;
    background: #FAF0E6;
    height: 65rpx;
    font-size: 30rpx;
    border-radius: 20rpx;
}
